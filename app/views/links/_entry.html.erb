<a href="http://www.google.com">
	<li class="single_entry">

		<ul>
			<li>

				<h3>
					<%= link_to link.title, url_with_protocol(link.url), :target => '_blank' %>
				</h3>
				<%= link_to "edit", edit_link_path(link), :class => "edit_link" %>
				<%= link_to "show", link_path(link), :class => "link" %>
			</li>

			<li>
				<%= "#{link.url} via @#{link.user.twitter_handle}"%>
			</li>

		</ul>
	</li>
</a>